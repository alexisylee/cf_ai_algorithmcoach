{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "algorithm-coach",
  "main": "src/server.ts",
  "compatibility_date": "2025-12-17",
  "compatibility_flags": ["nodejs_compat"],
  "ai": {
    "binding": "AI"
  },
  "assets": {
    "directory": "public"
  },
  "durable_objects": {
    "bindings": [
      {
        "name": "AlgorithmCoach",
        "class_name": "AlgorithmCoach"
      }
    ]
  },
  "migrations": [
    {
      "tag": "v1",
      "new_sqlite_classes": ["AlgorithmCoach"]
    }
  ]
}
